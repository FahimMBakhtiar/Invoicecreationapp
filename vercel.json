{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "api/pdf.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/generate-pdf",
      "destination": "/api/pdf"
    }
  ]
}

